<% layout('../partials/pages-layout') %>
<div class="page-content">
    <div class="page-header"></div><!-- /.page-header -->
    <div class="row">
        <div class="col-xs-12">
            <div class="widget-box">
                <div class="widget-header">
                    <h4 class="widget-title">网关基本信息</h4>
                </div>
                <div class="widget-body">
                    <div class="widget-main no-padding">
                        <table class="table table-striped table-bordered">
                            <tbody>
                                <tr>
                                    <td class="col-sm-3"><span class="pull-right"> IP地址: </span></td>
                                    <td class="col-sm-9"><%=ip%></td>
                                </tr>
                                <tr>
                                    <td class="col-sm-3"><span class="pull-right"> 子网掩码: </span></td>
                                    <td class="col-sm-9"><%=netmask%></td>
                                </tr>
                                <tr>
                                    <td class="col-sm-3"><span class="pull-right"> 默认路由: </span></td>
                                    <td class="col-sm-9"><%=gateway%></td>
                                </tr>
                                <tr>
                                    <td class="col-sm-3"><span class="pull-right"> 通讯端口: </span></td>
                                    <td class="col-sm-9"><%=thriftPort%></td>
                                </tr>
                                <tr>
                                    <td class="col-sm-3"><span class="pull-right"> 远端通讯IP地址: </span></td>
                                    <td class="col-sm-9"><%=thriftRemoteIp%></td>
                                </tr>
                                <tr>
                                    <td class="col-sm-3"><span class="pull-right"> 远端通讯端口: </span></td>
                                    <td class="col-sm-9"><%=thriftRemotePort%></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>


        </div><!-- /.col -->
    </div><!-- /.row -->
</div><!-- /.page-content -->